Algoritmo "exercicio2"

Var
valorDeposito: REAL;
saldoInicial: REAL;

Inicio
      escreval("Digite o valor inicial da conta: ")
      leia(saldoInicial)
      escreval("Digite o valor do deposito: ")
      leia(valorDeposito)
      escreval("O saldo da conta deposi do deposito vai ficar: ", saldoInicial + valorDeposito)

Fimalgoritmo